
 # eg:dsd-under-five_mortality_rate
eg:dsd-under-five_mortality_rate a qb:DataStructureDefinition;
    qb:component
    [ qb:dimension eg:refArea;               qb:order 3 ],
    [ qb:dimension eg:refPeriod;               qb:order 2 ],
    [ qb:dimension eg:peopleUnder5;               qb:order 2 ],
    qb:component
    [ qb:measure eg-measure:under-five-mortality-rate ];
    qb:component
    [ qb:attribute sdmx-attribute:unitMeasure ;
    sdmx-attribute:unitMeasure eg:per100000livebirths; ] ; 
.

